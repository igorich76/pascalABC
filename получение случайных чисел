program pr1;
var 
  i,j:integer;
  s,s2:real;
begin
  i:=Random(100);//   по моему, будет число от 0..99 (или 100?)
  Writeln('i = ',i);
  j:=Random(20,50); //задаём нужный диапазон
  Writeln('j = ',j);
  s:=Random();//будет число 0..1, функция Random без параметра!
  Writeln('s = ',s:5:2);
  s2:=Random() * 10 + 10;//если нам нужно сделать вещественное рандомное число, но нужного нам диапазона
  Writeln('s2 = ',s2:5:2);
end.
