begin 
var a,b,c,d,ur: integer; 
read(ur); 
a:= (45*ur)+510; {длительность уроков в зависимости от номера урока+время от начала суток} 
b:= (10*(ur-1));{длительность перемены, в зависимости от номера перемены (перемены меньше на один, чем уроков)} 
c:= (a+b) div 60; 
d:= (a+b) mod 60; 
print(c,d); 
End. 
{//Комменты: 510 - это 8-30 в минутах от начала суток.} 
